{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "Issuer": "https://localhost:7081",
    "Audience": "https://localhost:7081",

    // Used to encrypt and decrypt the jwt token
    "SigningKey": "some-signing-key-here_some-signing-key-here_some-signing-key-here",

    // token expiration time
    "ExpirationSeconds": 3600
  }
}